{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "SST Dev Debug",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v22.21.1/bin/node",
      "runtimeArgs": [
        "--inspect-brk",
        "./node_modules/.bin/sst",
        "dev"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true
    }
  ]
}
